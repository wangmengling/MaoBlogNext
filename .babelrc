{
  "presets": [
    "next/babel"
  ]
  ,
  "plugins": [
    ["import", { libraryName: "antd", style: "true" }] // `style: true` for less
    ,[
      "module-resolver", {
        "root": ["."],
        "alias": {
          "styles": "./styles"
        },
        "cwd": "babelrc"
    }]
    ,[
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$", "less$"]
      }
    ]
  ]
}